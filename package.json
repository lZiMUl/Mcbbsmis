  "scripts": {
    "init": "node . && npm run start",
    "start": "node .",
    "config": "node . --configure-panel && npm run start",
    "lint": "npx eslint --fix",
    "build": "npm run lint && npm run build:css && npm run build:client && npm run build:server",
    "build:css": "npx sass ./src/client/scss:public/css",
    "build:client": "npx tsc -p ./tsconfig.client.json",
    "build:server": "npx tsc -p ./tsconfig.server.json",
    "typedoc:client": "npx typedoc --options ./typedoc.client.json",
    "typedoc:server": "npx typedoc --options ./typedoc.server.json",
    "dev": "npm run build && npm run start",
    "test": "echo Done",
    "install:china": "npm install --legacy-peer-deps --register=https://registry.npmmirror.com",
    "update:china": "npm update --legacy-peer-deps --register=https://registry.npmmirror.com"
  },
  "exports": {
    ".": {
      "default": {
        "default": "./build/index.js",
        "require": "./build/index.js",
        "types": "./lib/index.d.ts"
      }
    }
  },
  "files": [
    "./build/",
    "./data/",
    "./lang",
    "./lib/",
    "./proto",
